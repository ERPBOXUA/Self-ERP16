<?xml version="1.0" encoding="UTF-8"?>
<odoo>


    <record id="account_move_line_analytic_link_view_form" model="ir.ui.view">
        <field name="name">account.move.line.analytic.link.view.form</field>
        <field name="model">account.move.line.analytic.link</field>
        <field name="arch" type="xml">
            <form>
                <sheet>
                    <group>
                        <field name="cash_flow_analytic_account_id"/>
                    </group>
                </sheet>
                <footer>
                    <button name="action_select_analytic_account" string="Select" type="object" class="btn-primary"/>
                    <button special="cancel" string="Close" class="btn-secondary"/>
                </footer>
            </form>
        </field>
    </record>


</odoo>
