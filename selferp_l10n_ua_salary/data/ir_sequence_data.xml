<?xml version="1.0" encoding="UTF-8"?>
<odoo noupdate="1">

    <record id="ir_sequence_hr_schedule" model="ir.sequence">
        <field name="name">Staff Schedule Number</field>
        <field name="code">hr.schedule.number.sequence</field>
        <field name="padding">8</field>
        <field name="number_next" eval="1"/>
        <field name="number_increment" eval="1"/>
        <field name="company_id" eval="False"/>
    </record>

    <record id="seq_hr_pdfo_report" model="ir.sequence">
        <field name="name">PDFO Report</field>
        <field name="code">hr.pdfo_report</field>
        <field name="prefix">ПДФО</field>
        <field name="padding">5</field>
        <field name="company_id" eval="False"/>
    </record>

</odoo>
